    "class Animal:\n",
    "    def __init__(self,name):\n",
    "        self.name = name \n",
    "\n",
    "    def show(self):\n",
    "        print(f\"Hello your name is {self.name} \")\n",
    "\n",
    "class Human(Animal):\n",
    "    def __init__(self,name, age): ## here super will target Animal class\n",
    "        super().__init__(name)\n",
    "        self.age = age\n",
    "    def show(self):\n",
    "        print(f\"Hello your name is {self.name} and your age is {self.age}\")\n",
    "        \n",
    "obj = Human(\"bholu\",16)\n",
    "obj.show()"
