    "## Multiple inheritance\n",
    "\n",
    "class Animal:\n",
    "    name1 = \"Lion\"\n",
    "\n",
    "class Human:\n",
    "    name2 = \"Bholu\"\n",
    "\n",
    "class Robot(Animal,Human):\n",
    "    name3 = \"chiti 2.0\"\n",
    "obj = Robot()\n",
    "print(obj.name1)\n",
    "print(obj.name2)\n",
    "print(obj.name3)"
   ]
